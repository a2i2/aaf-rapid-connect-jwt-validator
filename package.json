{
  "name": "aaf-rapid-connect-jwt-validator",
  "version": "0.0.0",
  "description": "Validate JWT claims according to AAF's recommendations.",
  "author": "DSTIL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstil/aaf-rapid-connect-jwt-validator.git"
  },
  "bugs": {
    "url": "https://github.com/dstil/aaf-rapid-connect-jwt-validator/issues"
  },
  "main": "dist",
  "scripts": {
